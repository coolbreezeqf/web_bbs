server {
    listen 80;
    server_name bbs.colbrze.xyz colbrze.xyz www.colbrze.xyz *.colbrze.xyz;
    location /images {
        alias /var/www/web_bbs/images;
    }

    location /static {
        alias /var/www/web_bbs/static;
    }

    location / {
        proxy_pass http://localhost:2000;
    }
}
